{
    "name": "pojoe",
    "version": "1.0.0",
    "description": "this is the main Pojoe (formerly Pojo Engine) library use it to develop pluggable Pojoe steps, \n this librarary provides some simple but useful steps (see below)",
    "main": "node ./test.js",
    "config": {
        "steps": [
            "PojoProducer",
            "PojoFilter",
            "PojoLogger",
            "PojoAttrSelector",
            "PojoLookup",
            "PojoTimer"
        ]
    },
    "scripts": {
        "build": "tsc",
        "pretest": "npm run doc",
        "test": "node ./steps/pojoetest --STR=Hello  --INT=123 --BOOL=true --NUM=3.14159265 --DEBUG",
        "predoc": "tsc",
        "doc": "node ./steps/pojoedoc"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/mbenzekri/pojoe.git"
    },
    "author": "Mohamed Ben Zekri",
    "license": "UNLICENSED",
    "bugs": {
        "url": "https://github.com/mbenzekri/pojoe/issues"
    },
    "homepage": "https://github.com/mbenzekri/pojoe#readme",
    "dependencies": {
        "uuid": "^3.3.2"
    },
    "devDependencies": {
        "@types/node": "^12.0.1"
    }
}
